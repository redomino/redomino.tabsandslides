<div tal:condition="view/data/omit_border"
     tal:attributes="class string:portlet portletSlideshow ${view/css_class}"
     i18n:domain="plone">

    <tal:events tal:define="objects view/getObjects">


        <ul class="imagegallery jcarousel-skin-plone" tal:define="objs view/getObjects" tal:condition="objs">
            <li tal:repeat="obj objs" tal:content="structure obj/@@gallery_adapter">gallery 1 content</li>
        </ul>

    </tal:events>

</div>

<dl tal:condition="not:view/data/omit_border"
    tal:attributes="class string:portlet portletTab ${view/css_class}"
    i18n:domain="plone"
    tal:define="collection_url view/collection_url">

    <dt class="portletHeader">
        <span class="portletTopLeft"></span>
        <a tal:attributes="href collection_url">
            <span tal:content="view/data/header" />
        </a>
        <span class="portletTopRight"></span>
    </dt>
    

    <dd tal:define="objects view/getObjects">

        <ul class="imagegallery jcarousel-skin-plone" tal:define="objs view/getObjects" tal:condition="objs">
            <li tal:repeat="obj objs" tal:content="structure obj/@@gallery_adapter">gallery 1 content</li>
        </ul>

    </dd>
    
    <dd class="portletFooter">
        <span class="portletBottomLeft"></span>
        <span>
        </span>
        <span class="portletBottomRight"></span>
    </dd>

</dl>
